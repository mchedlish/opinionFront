js",["124","125","126"],"import React from 'react';\nimport './Post.css'\nimport Book from '../assets/icons/open-book.png'\nimport {Link} from \"react-router-dom\";\n\nconst Post = ({category, title, excerpt, photo, singleBlog, scroll, direct}) => {\n    \nconst styles = {\n    backgroundColor:\"black\",\n    objectFit:'cover',\n    backgroundRepeat:\"no-repeat\",\n    width: '100%',\n    height:'240px'\n}\n\n\n\n\n return (\n        <div className='post col-md-12' >\n        <div onClick={direct}>\n        <Link\n        style={{textDecoration:'none'}} onClick={scroll}>\n        <div className='image'onClick={e=>singleBlog(photo)} >\n        <img src={\"http://localhost:8000/api/blog/photo/\"+`${photo}`} style={styles}/>\n        \n        <p className='category'>{category}</p>\n        <p className='headline'>{title}</p>\n        <p className='excerpt' dangerouslySetInnerHTML={{__html:excerpt}}></p>\n        <div className='reading'><span><img className='clock' src={Book}/><span className='minutes'></span></span> წაიკითხეთ მეტი...</div>\n        </div>\n        </Link>\n        </div>\n        </div>\n    );\n};\n\nexport default Post; ","/Users/nikolozmchedlishvili/Desktop/opinion/frontend/src/components/auth/SignUpComponent.js",[],"/Users/nikolozmchedlishvili/Desktop/opinion/frontend/src/actions/auth.js",[],"/Users/nikolozmchedlishvili/Desktop/opinion/frontend/src/components/auth/SignInComponent.js",["127"],"import React, { useState } from 'react';\nimport { signin, authenticate, isAuth } from '../../actions/auth';\n\n\n\nconst SigninComponent = (props) => {\n    const [values, setValues] = useState({\n        email: 'ryan@gmail.com',\n        password: 'rrrrrr',\n        error: '',\n        loading: false,\n        message: '',\n        showForm: true\n    });\n\n    const { email, password, error, loading, message, showForm } = values;\n\n    const handleSubmit = e => {\n        e.preventDefault();\n        // console.table({ name, email, password, error, loading, message, showForm });\n        setValues({ ...values, loading: true, error: false });\n        const user = { email, password };\n\n        signin(user).then(data => {\n            if (data.error) {\n                setValues({ ...values, error: data.error, loading: false });\n            } else {\n                // save user token to cookie\n                // save user info to localstorage\n                // authenticate user\n                authenticate(data, () => {\nif (isAuth()&&isAuth().role==1) {\n\n                  props.history.push(`/admin`);}\n                  else {\n\n                    props.history.push('/user')\n                  }\n                });\n            }\n        });\n    };\n\n    const handleChange = name => e => {\n        setValues({ ...values, error: false, [name]: e.target.value });\n    };\n\n    const showLoading = () => (loading ? <div className=\"alert alert-info\">Loading...</div> : '');\n    const showError = () => (error ? <div className=\"alert alert-danger\">{error}</div> : '');\n    const showMessage = () => (message ? <div className=\"alert alert-info\">{message}</div> : '');\n\n    const signinForm = () => {\n        return (\n            <form onSubmit={handleSubmit}>\n                <div className=\"form-group\">\n                    <input\n                        value={email}\n                        onChange={handleChange('email')}\n                        type=\"email\"\n                        className=\"form-control\"\n                        placeholder=\"Type your email\"\n                    />\n                </div>\n\n                <div className=\"form-group\">\n                    <input\n                        value={password}\n                        onChange={handleChange('password')}\n                        type=\"password\"\n                        className=\"form-control\"\n                        placeholder=\"Type your password\"\n                    />\n                </div>\n\n                <div>\n                    <button className=\"btn btn-primary\">Signin</button>\n                </div>\n            </form>\n        );\n    };\n\n    return (\n        <React.Fragment>\n            {showError()}\n            {showLoading()}\n            {showMessage()}\n            {showForm && signinForm()}\n        </React.Fragment>\n    );\n};\n\nexport default SigninComponent;","/Users/nikolozmchedlishvili/Desktop/opinion/frontend/src/admin/admin.js",[],"/Users/nikolozmchedlishvili/Desktop/opinion/frontend/src/user/index.js",[],"/Users/nikolozmchedlishvili/Desktop/opinion/frontend/src/components/auth/Private.js",["128"],"import React, { useEffect } from 'react';\nimport {withRouter} from 'react-router-dom'\n\nimport { isAuth } from '../../actions/auth';\n\nconst Private = (props, { children }) => {\n    useEffect(() => {\n        if (!isAuth()) {\n            props.history.push(`/signin`);\n        }\n    }, []);\n    return <React.Fragment>{children}</React.Fragment>;\n};\n\nexport default withRouter(Private);","/Users/nikolozmchedlishvili/Desktop/opinion/frontend/src/admin/crud/category-tag.js",[],"/Users/nikolozmchedlishvili/Desktop/opinion/frontend/src/components/crud/Category.js",["129","130","131"],"import React, { useState, useEffect } from 'react';\n\n\nimport {getCookie } from '../../actions/auth';\nimport { create, getCategories, removeCategory } from '../../actions/category';\n\nconst Category = () => {\n    const [values, setValues] = useState({\n        name: '',\n        error: false,\n        success: false,\n        categories: [],\n        removed: false,\n        reload:false\n    });\n\n    const { name, error, success, categories, removed, reload } = values;\n    const token = getCookie('token');\n    useEffect(() => {\n        loadCategories();\n    }, [reload]);\n\n    const loadCategories = () => {\n        getCategories().then(data => {\n            if (data.error) {\n                console.log(data.error);\n            } else {\n                setValues({ ...values, categories: data });\n            }\n        });\n    };\n\n    const showCategories = () => {\n        return categories.map((c, i) => {\n            return (\n                <button\n                    onDoubleClick={() => deleteConfirm(c.slug)}\n                    title=\"Double click to delete\"\n                    key={i}\n                    className=\"btn btn-outline-primary mr-1 ml-1 mt-3\"\n                >\n                    {c.name}\n                </button>\n            );\n        });\n    };\n\n    const deleteConfirm = slug => {\n        let answer = window.confirm('Are you sure you want to delete this category?');\n        if (answer) {\n            deleteCategory(slug);\n        }\n    };\n\n    const deleteCategory = slug => {\n        // console.log('delete', slug);\n        removeCategory(slug, token).then(data => {\n            if (data.error) {\n                console.log(data.error);\n            } else {\n                setValues({ ...values, error: false, success: false, name: '', removed: !removed, reload: !reload });\n            }\n        });\n    };\n\n\n\n\n    const clickSubmit = e => {\n        e.preventDefault();\n        // console.log('create category', name);\n        create({ name }, token).then(data => {\n            if (data.error) {\n                setValues({ ...values, error: data.errer, success: false });\n            } else {\n                setValues({ ...values, error: false, success: true, name: '', reload:!reload });\n            }\n        });\n    };\n\n    const handleChange = e => {\n        setValues({ ...values, name: e.target.value, error: false, success: false, removed: '' });\n    };\n\n    const newCategoryForm = () => (\n        <form onSubmit={clickSubmit}>\n            <div className=\"form-group\">\n                <label className=\"text-muted\">Name</label>\n                <input type=\"text\" className=\"form-control\" onChange={handleChange} value={name} required />\n            </div>\n            <div>\n                <button type=\"submit\" className=\"btn btn-primary\">\n                    Create\n                </button>\n            </div>\n        </form>\n    );\n\n    return <React.Fragment>\n    {newCategoryForm()}\n    <div>{showCategories()}</div>\n    </React.Fragment>;\n};\n\nexport default Category;\n","/Users/nikolozmchedlishvili/Desktop/opinion/frontend/src/actions/category.js",[],"/Users/nikolozmchedlishvili/Desktop/opinion/frontend/src/components/crud/Tag.js",["132","133","134"],"import React, { useState, useEffect } from 'react';\n\n\nimport {getCookie } from '../../actions/auth';\nimport { create, getTags, removeTag } from '../../actions/tag';\n\nconst Tag = () => {\n    const [values, setValues] = useState({\n        name: '',\n        error: false,\n        success: false,\n        tags: [],\n        removed: false,\n        reload:false\n    });\n\n    const { name, error, success, tags, removed, reload } = values;\n    const token = getCookie('token');\n    useEffect(() => {\n        loadCategories();\n    }, [reload]);\n\n    const loadCategories = () => {\n        getTags().then(data => {\n            if (data.error) {\n                console.log(data.error);\n            } else {\n                setValues({ ...values, tags: data });\n            }\n        });\n    };\n\n    const showTags = () => {\n        return tags.map((c, i) => {\n            return (\n                <button\n                    onDoubleClick={() => deleteConfirm(c.slug)}\n                    title=\"Double click to delete\"\n                    key={i}\n                    className=\"btn btn-outline-primary mr-1 ml-1 mt-3\"\n                >\n                    {c.name}\n                </button>\n            );\n        });\n    };\n\n    const deleteConfirm = slug => {\n        let answer = window.confirm('Are you sure you want to delete this category?');\n        if (answer) {\n            deleteTag(slug);\n        }\n    };\n\n    const deleteTag = slug => {\n        // console.log('delete', slug);\n        removeTag(slug, token).then(data => {\n            if (data.error) {\n                console.log(data.error);\n            } else {\n                setValues({ ...values, error: false, success: false, name: '', removed: !removed, reload: !reload });\n            }\n        });\n    };\n\n\n\n\n    const clickSubmit = e => {\n        e.preventDefault();\n        // console.log('create category', name);\n        create({ name }, token).then(data => {\n            if (data.error) {\n                setValues({ ...values, error: data.errer, success: false });\n            } else {\n                setValues({ ...values, error: false, success: true, name: '', reload:!reload });\n            }\n        });\n    };\n\n    const handleChange = e => {\n        setValues({ ...values, name: e.target.value, error: false, success: false, removed: '' });\n    };\n\n    const newTagForm = () => (\n        <form onSubmit={clickSubmit}>\n            <div className=\"form-group\">\n                <label className=\"text-muted\">Name</label>\n                <input type=\"text\" className=\"form-control\" onChange={handleChange} value={name} required />\n            </div>\n            <div>\n                <button type=\"submit\" className=\"btn btn-primary\">\n                    Create\n                </button>\n            </div>\n        </form>\n    );\n\n    return <React.Fragment>\n    {newTagForm()}\n    <div>{showTags()}</div>\n    </React.Fragment>;\n};\n\nexport default Tag;\n","/Users/nikolozmchedlishvili/Desktop/opinion/frontend/src/actions/tag.js",[],["135","136"],"/Users/nikolozmchedlishvili/Desktop/opinion/frontend/src/admin/crud/blog.js",["137"],"import React from 'react-router-dom'\n//import Admin from '../../components/auth/Admin';\nimport BlogCreate from '../../components/crud/BlogCreate';\nimport {Link} from 'react-router-dom' \n