ories()}</ul>\n                    </div>\n                    <div>\n                        <h5>Tags</h5>\n                        <hr />\n                        <ul style={{ maxHeight: '200px', overflowY: 'scroll' }}>{showTags()}</ul>\n                    </div>\n                </div>\n            </div>\n        </div>\n    );\n};\n\nCreateBlog.modules = {\n    toolbar: [\n        [{ header: '1' }, { header: '2' }, { header: [3, 4, 5, 6] }, { font: [] }],\n        [{ size: [] }],\n        ['bold', 'italic', 'underline', 'strike', 'blockquote'],\n        [{ list: 'ordered' }, { list: 'bullet' }],\n        ['link', 'image', 'video'],\n        ['clean'],\n        ['code-block']\n    ]\n};\n\nCreateBlog.formats = [\n    'header',\n    'font',\n    'size',\n    'bold',\n    'italic',\n    'underline',\n    'strike',\n    'blockquote',\n    'list',\n    'bullet',\n    'link',\n    'image',\n    'video',\n    'code-block'\n];\n\nexport default withRouter(CreateBlog);","/Users/nikolozmchedlishvili/Desktop/opinion/frontend/src/components/auth/Admin.js",["143","144"],"/Users/nikolozmchedlishvili/Desktop/opinion/frontend/src/actions/blog.js",[],{"ruleId":"145","replacedBy":"146"},{"ruleId":"147","replacedBy":"148"},{"ruleId":"149","severity":1,"message":"150","line":14,"column":15,"nodeType":"151","messageId":"152","endLine":14,"endColumn":23},{"ruleId":"149","severity":1,"message":"153","line":16,"column":13,"nodeType":"151","messageId":"152","endLine":16,"endColumn":21},{"ruleId":"154","severity":1,"message":"155","line":89,"column":85,"nodeType":"156","endLine":89,"endColumn":129},{"ruleId":"154","severity":1,"message":"155","line":13,"column":44,"nodeType":"156","endLine":13,"endColumn":61},{"ruleId":"154","severity":1,"message":"155","line":15,"column":19,"nodeType":"156","endLine":15,"endColumn":40},{"ruleId":"154","severity":1,"message":"155","line":16,"column":19,"nodeType":"156","endLine":16,"endColumn":39},{"ruleId":"154","severity":1,"message":"155","line":17,