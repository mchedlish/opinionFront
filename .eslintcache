  <Link style={{color:props.color? \"#1c1c1c\":'white'}} onClick={()=>props.fetchPolitics()}><li className={showlistOne}>პოლიტიკა</li></Link>\n        <Link style={{color:props.color? \"#1c1c1c\":'white'}} onClick={()=>props.fetchEconomics()}><li className={showlistTwo}>ეკონომიკა</li></Link>\n        <Link style={{color:props.color? \"#1c1c1c\":'white'}} onClick={()=>props.fetchWorld()}><li className={showlistThree}>მსოფლიო</li></Link>\n        <Link style={{color:props.color? \"#1c1c1c\":'white'}} onClick={()=>props.fetchTechnologies()}><li className={showlistFour}>ტექნოლოგიები</li></Link>\n        <Link style={{color:props.color? \"#1c1c1c\":'white'}} onClick={()=>props.fetchTrends()} ><li className={showlistFive}>ტრენდები</li></Link>\n        </ul>\n            </div>\n    );\n};\n\n\nconst mapDispatchToProps = dispatch => {\n    return {\n      fetchPolitics: () => dispatch(fetchPolitics()),\n      fetchEconomics: () => dispatch(fetchEconomics()),\n      fetchWorld:() => dispatch(fetchWorld()),\n      fetchTechnologies:() => dispatch(fetchTechnologies()),\n      fetchTrends:() => dispatch(fetchTrends())\n    }\n  }\nexport default connect(null, mapDispatchToProps)(BottomMenu);","/Users/nikolozmchedlishvili/Desktop/opinion/frontend/src/components/Search.js",["141"],"/Users/nikolozmchedlishvili/Desktop/opinion/frontend/src/components/Post.js",["142","143","144"],"import React from 'react';\nimport './Post.css'\nimport Book from '../assets/icons/open-book.png'\nimport {Link} from \"react-router-dom\";\n\nconst Post = ({category, title, excerpt, photo, singleBlog, scroll, direct}) => {\n    \nconst styles = {\n    backgroundColor:\"black\",\n    objectFit:'cover',\n    backgroundRepeat:\"no-repeat\",\n    width: '100%',\n    height:'240px'\n}\n\n\n\n\n return (\n        <div className='post col-md-12' >\n        <div onClick={direct}>\n        <Link\n        style={{textDecoration:'none'}} onClick={scroll}>\n        <div className='image'onClick={e=>singleBlog(photo)} >\n        <img src={\"http://localhost:8000/api/blog/photo/\"+`${photo}`} style={styles}/>\n        \n        <p className='category'>{category}</p>\n        <p className='headline'>{title}</p>\n        <p className='excerpt' dangerouslySetInnerHTML={{__html:excerpt}}></p>\n        <div className='reading'><span><img className='clock' src={Book}/><span className='minutes'></span></span> წაიკითხეთ მეტი...</div>\n        </div>\n        </Link>\n        </div>\n        </div>\n    );\n};\n\nexport default Post; ","/Users/nikolozmchedlishvili/Desktop/opinion/frontend/src/components/auth/SignUpComponent.js",[],"/Users/nikolozmchedlishvili/Desktop/opinion/frontend/src/actions/auth.js",[],"/Users/nikolozmchedlishvili/Desktop/opinion/frontend/src/components/auth/SignInComponent.js",["145"],"import React, { useState } from 'react';\nimport { signin, authenticate, isAuth } from '../../actions/auth';\n\n\n\nconst SigninComponent = (props) => {\n    const [values, setValues] = useState({\n        email: 'ryan@gmail.com',\n        password: 'rrrrrr',\n        error: '',\n        loading: false,\n        message: '',\n        showForm: true\n    });\n\n    const { email, password, error, loading, message, showForm } = values;\n\n    const handleSubmit = e => {\n        e.preventDefault();\n        // console.table({ name, email, password, error, loading, message, showForm });\n        setValues({ ...values, loading: true, error: false });\n        const user = { email, password };\n\n        signin(user).then(data => {\n            if (data.error) {\n                setValues({ ...values, error: data.error, loading: false });\n            } else {\n                // save user token to cookie\n                // save user info to localstorage\n                // authenticate user\n                authenticate(data, () => {\nif (isAuth()&&isAuth().role==1) {\n\n                  props.history.push(`/admin`);}\n                  else {\n\n                    props.history.push('/user')\n                  }\n                });\n            }\n        });\n    };\n\n    const handleChange = name => e => {\n        setValues({ ...values, error: false, [name]: e.target.value });\n    };\n\n    const showLoading = () => (loading ? <div className=\"alert alert-info\">Loading...</div> : '');\n    const showError = () => (error ? <div className=\"alert alert-danger\">{error}</div> : '');\n    const showMessage = () => (message ? <div className=\"alert alert-info\">{message}</div> : '');\n\n    const signinForm = () => {\n        return (\n            <form onSubmit={handleSubmit}>\n                <div className=\"form-group\">\n                    <input\n                        value={email}\n                        onChange={handleChange('email')}\n                        type=\"email\"\n                        className=\"form-control\"\n                        placeholder=\"Type your email\"\n                    />\n                </div>\n\n                <div className=\"form-group\">\n                    <input\n                        value={password}\n                        onChange={handleChange('password')}\n                        type=\"password\"\n                        className=\"form-control\"\n                        placeholder=\"Type your password\"\n                    />\n                </div>\n\n                <div>\n                    <button className=\"btn btn-primary\">Signin</button>\n                </div>\n            </form>\n        );\n    };\n\n    return (\n        <React.Fragment>\n            {showError()}\n            {showLoading()}\n            {showMessage()}\n            {showForm && signinForm()}\n        </React.Fragment>\n    );\n};\n\nexport default SigninComponent;","/Users/nikolozmchedlishvili/Desktop/opinion/frontend/src/admin/admin.js",[],"/Users/nikolozmchedlishvili/Desktop/opinion/frontend/src/user/index.js",[],"/Users/nikolozmchedlishvili/Desktop/opinion/frontend/src/components/auth/Pr