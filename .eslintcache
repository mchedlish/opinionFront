Values({ ...values, error: data.errer, success: false });\n            } else {\n                setValues({ ...values, error: false, success: true, name: '', reload:!reload });\n            }\n        });\n    };\n\n    const handleChange = e => {\n        setValues({ ...values, name: e.target.value, error: false, success: false, removed: '' });\n    };\n\n    const newTagForm = () => (\n        <form onSubmit={clickSubmit}>\n            <div className=\"form-group\">\n                <label className=\"text-muted\">Name</label>\n                <input type=\"text\" className=\"form-control\" onChange={handleChange} value={name} required />\n            </div>\n            <div>\n                <button type=\"submit\" className=\"btn btn-primary\">\n                    Create\n                </button>\n            </div>\n        </form>\n    );\n\n    return <React.Fragment>\n    {newTagForm()}\n    <div>{showTags()}</div>\n    </React.Fragment>;\n};\n\nexport default Tag;\n","/Users/nikolozmchedlishvili/Desktop/opinion/frontend/src/actions/tag.js",[],"/Users/nikolozmchedlishvili/Desktop/opinion/frontend/src/admin/crud/blog.js",["153"],"import React from 'react-router-dom'\n//import Admin from '../../components/auth/Admin';\nimport BlogCreate from '../../components/crud/BlogCreate';\nimport {Link} from 'react-router-dom' \n\nconst Blog = () => {\n    return (\n        \n            \n                <div className=\"container-fluid\">\n                    <div className=\"row\">\n                        <div className=\"col-md-12 pt-5 pb-5\">\n                            <h2>Create a new blog</h2>\n                        </div>\n                        <div className=\"col-md-12\">\n                            <BlogCreate />\n                        </div>\n                    </div>\n                </div>\n            \n    \n    );\n};\n\nexport default Blog;","/Users/nikolozmchedlishvili/Desktop/opinion/frontend/src/components/crud/BlogCreate.js",["154","15